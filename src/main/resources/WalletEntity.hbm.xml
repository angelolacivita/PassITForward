<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gc.models.WalletEntity" table="wallet" schema="PassITForward">
        <id name="walletId">
            <column name="walletID" sql-type="int(10) unsigned"/>
        </id>
        <property name="walletValue">
            <column name="walletValue" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>